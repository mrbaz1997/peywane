{
  "name": "peywane",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run build",
    "build": "npm run install-client && cd client && npm run build",
    "install-client": "cd client && npm install",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "client": "cd client && npm start",
    "test": "jest --passWithNoTests"
  },
  "main": "server.js",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "pg": "^8.13.0",
    "pug": "^2.0.4",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "jest": "^29.7.0"
  }
}
